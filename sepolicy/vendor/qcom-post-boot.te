allow qcom-post-boot init:unix_stream_socket connectto;
allow qcom-post-boot post_boot_prop:property_service set;
allow qcom-post-boot property_socket:sock_file write;
allow qcom-post-boot sysfs_socinfo:file write;
allow qcom-post-boot system_prop:property_service set;
allow qcom-post-boot vendor_toolbox_exec:file execute_no_trans;
