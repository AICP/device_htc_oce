# qcom-post-boot service
type qcom-post-boot, domain;
type qcom-post-boot_exec, exec_type, file_type, vendor_file_type;
# Started by init
init_daemon_domain(qcom-post-boot) 

allow qcom-post-boot block_device:dir r_dir_perms;
allow qcom-post-boot ctl_start_prop:property_service set;
allow qcom-post-boot ctl_stop_prop:property_service set;

dontaudit qcom-post-boot bootloader_boot_reason_prop:file r_file_perms;
dontaudit qcom-post-boot firstboot_prop:file r_file_perms;
dontaudit qcom-post-boot last_boot_reason_prop:file r_file_perms;
dontaudit qcom-post-boot netd_stable_secret_prop:file r_file_perms;
dontaudit qcom-post-boot pm_prop:file { read open };
dontaudit qcom-post-boot serialno_prop:file r_file_perms;
