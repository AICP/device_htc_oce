type init_usbdiag, domain;
type init_usbdiag_exec, exec_type, vendor_file_type, file_type;
init_daemon_domain(init_usbdiag)

allow init_usbdiag proc:file getattr;
allow init_usbdiag proc_iomem:file getattr;
allow init_usbdiag proc_meminfo:file getattr;
allow init_usbdiag proc_sysrq:file getattr;

allow init_usbdiag vendor_shell_exec:file rx_file_perms;
allow init_usbdiag vendor_toolbox_exec:file rx_file_perms;
allow init_usbdiag domain:dir { getattr search };
allow init_usbdiag domain:file { read open };

allow init_usbdiag kernel:process setsched;
