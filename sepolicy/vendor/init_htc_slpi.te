type init_htc_slpi, domain;
type init_htc_slpi_exec, exec_type, vendor_file_type, file_type;
init_daemon_domain(init_htc_slpi)

allow init_htc_slpi proc:file getattr;
allow init_htc_slpi proc_iomem:file getattr;
allow init_htc_slpi proc_meminfo:file getattr;
allow init_htc_slpi proc_sysrq:file getattr;

allow init_htc_slpi vendor_shell_exec:file rx_file_perms;
allow init_htc_slpi vendor_toolbox_exec:file rx_file_perms;
allow init_htc_slpi domain:dir { getattr search };
allow init_htc_slpi domain:file { read open };

allow init_htc_slpi kernel:process setsched;
