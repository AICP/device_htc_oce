type firmware-links, domain;
type firmware-links_exec, exec_type, vendor_file_type, file_type;
init_daemon_domain(firmware-links) 

allow firmware-links vendor_shell_exec:file rx_file_perms;
allow firmware-links vendor_toolbox_exec:file rx_file_perms;
allow firmware-links domain:dir { getattr search };
allow firmware-links domain:file { read open };
allow firmware-links toolbox_exec:file rx_file_perms;
allow firmware-links shell_exec:file { execute_no_trans rx_file_perms };
allow firmware-links system_file:file { execute_no_trans rx_file_perms };
