type foreground_sh, domain;
type foreground_sh_exec, exec_type, file_type, vendor_file_type;
# Started by init
init_daemon_domain(foreground_sh)

allow foreground_sh init:dir { getattr search };
allow foreground_sh init:file { open read };
allow foreground_sh kernel:dir { getattr search };
allow foreground_sh kernel:file { open read };
allow foreground_sh vendor_toolbox_exec:file execute_no_trans;
allow foreground_sh ueventd:dir { getattr search };
allow foreground_sh ueventd:file { open read };
allow foreground_sh vendor_init:dir { getattr search };
allow foreground_sh vendor_init:file { open read };
allow foreground_sh logd:dir { getattr search };
allow foreground_sh logd:file { open read };
allow foreground_sh tee:dir { getattr search };
allow foreground_sh tee:file { open read };
allow foreground_sh hwservicemanager:dir { getattr search };
allow foreground_sh hwservicemanager:file { open read };
allow foreground_sh servicemanager:dir { getattr search };
allow foreground_sh servicemanager:file { open read };
allow foreground_sh vndservicemanager:dir { search getattr };
allow foreground_sh vndservicemanager:file { open read };
allow foreground_sh hal_keymaster_default:dir { getattr search };
allow foreground_sh hal_keymaster_default:file { open read };
allow foreground_sh vold:dir { getattr search };
allow foreground_sh vold:file { open read };
allow foreground_sh netd:dir { getattr search };
allow foreground_sh netd:file { open read };
allow foreground_sh mm-pp-daemon:dir { getattr search };
allow foreground_sh mm-pp-daemon:file { open read };
allow foreground_sh zygote:dir { getattr search };
allow foreground_sh zygote:file { open read };
