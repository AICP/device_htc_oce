dontaudit init shell_exec:file *;

allow init sysfs:file setattr;
allow init vendor_radio_data_file:dir mounton;
allow init ion_device:chr_file { ioctl open read };
allow init sysfs_fpc:file { open read write };
allow init sysfs_usb_supply:file { open read write };
allow init system_server:binder { call transfer };
allow init tee_device:chr_file ioctl;
