# qcom service
type qcom, domain;
type qcom_exec, exec_type, file_type, vendor_file_type;
# Started by init
init_daemon_domain(qcom)

allow qcom proc_net:file { open write };
allow qcom self:capability { chown net_admin fowner fsetid };
allow qcom unlabeled:file getattr;
allow qcom vendor_radio_data_file:dir { create setattr getattr add_name open read remove_name rmdir write };
allow qcom vendor_radio_data_file:file { getattr setattr open write };
allow qcom vendor_toolbox_exec:file execute_no_trans;
allow qcom radio_data_file:file { open write };
allow qcom proc:file { open write }; 
